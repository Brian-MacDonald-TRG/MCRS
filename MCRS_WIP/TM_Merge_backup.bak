			</tools-setup-item>
			<tools-setup-item>
				<name>Jack</name>
			</tools-setup-item>
			<tools-setup-item>
				<name>Jack Stands</name>
			</tools-setup-item>
			<tools-setup-item>
				<name>Shop Equipment, Welding</name>
				<itemref>
					<xref itemid="S00007-9-2320-451-TOOL-2" wpid="S00007-9-2320-451"/>
