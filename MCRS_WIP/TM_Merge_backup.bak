	</wpidinfo>
	<initial_setup>
		<tools>
			<tools-setup-item>
				<name>Tool Kit, General Mechanics</name>
				<itemref><xref itemid="S00007-9-2320-451-TOOL-4" wpid="S00007-9-2320-451"/>
				</itemref>
			</tools-setup-item>
		</tools>
		<mtrlpart>
			<mtrlpart-setup-item>
				<name>Washer, lock (2) (MS35333-40)</name>
			</mtrlpart-setup-item>
	
			<mtrlpart-setup-item>
				<name>Dry Cleaning Solvent (item 14, WP 0085)</name>
			</mtrlpart-setup-item>
			<mtrlpart-setup-item>
				<name>Rags (item 10, WP 0085)</name>
			</mtrlpart-setup-item>
			<mtrlpart-setup-item>
				<name>Brush, scrub (item 2, WP 0085)</name>
			</mtrlpart-setup-item>
			<mtrlpart-setup-item>
				<name>Thread sealing compound</name>
			</mtrlpart-setup-item>
