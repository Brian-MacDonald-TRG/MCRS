	<initial_setup>
		<tools>
			<tools-setup-item>
				<name>Tool Kit, Mechanics General</name>
				<itemref>
					<xref itemid="S00007-9-2320-451-TOOL-4" wpid="S00007-9-2320-451"/>
				</itemref>
			</tools-setup-item>
		</tools>
		<persnreq>
			<persnreq-setup-item>
				<name>Motor Transport Operator</name>
				<mos>88M</mos>
				<qty>1</qty>
			</persnreq-setup-item>
